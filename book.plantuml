@startuml first

class Autor {
    -name String
    -age Int
    -address String
    -construtor(name, age , address)
    +print()
}

class Book {
    -title String
    -year Int
    -editor String
    -pages Int
    -price Float
    -autor Autor
    -construtor(title,year,editor,pages,price)
    +print()
}

Book *-- Autor

@enduml